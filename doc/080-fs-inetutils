# fs:		inetutils-1.9.4
# sauce:	http://ftp.gnu.org/gnu/inetutils/inetutils-1.9.4.tar.xz

# 1
./configure \
 --prefix=/usr \
 --localstatedir=/var \
 --disable-logger \
 --disable-whois \
 --disable-rcp \
 --disable-rexec \
 --disable-rlogin \
 --disable-rsh \
 --disable-servers

# 2
make && make install
