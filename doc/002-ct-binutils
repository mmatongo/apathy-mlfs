# ct:		binutils-2.32
# source:	http://ftp.gnu.org/gnu/binutils/binutils-2.32.tar.xz

# 1
mkdir -v build && cd build

# 2
../configure \
 --prefix=/cross-tools \
 --target=${MLFS_TARGET} \
 --with-sysroot=/cross-tools/${MLFS_TARGET} \
 --disable-nls \
 --disable-multilib \
 --disable-werror \
 --enable-deterministic-archives \
 --disable-compressed-debug-sections 

# 3
make configure-host

# 4
make && make install
